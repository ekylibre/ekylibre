        <variable name="v_other_receivables" value="financial_year.sum_entry_items_with_mandatory_line(:balance_sheet, :other_receivables)"/>
        <variable name="v_other_receivable_provisions" value="financial_year.sum_entry_items_with_mandatory_line(:balance_sheet, :other_receivable_provisions)"/>
