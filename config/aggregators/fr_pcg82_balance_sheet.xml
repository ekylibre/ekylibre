            <cell name="previous_depreciated_balance" value="p_v_client_receivable_provisions" if="previous_financial_year"/>
            <cell name="total_balance" value="v_client_receivables - v_client_receivable_provisions" />
            <cell name="previous_total_balance" value="p_v_client_receivables - p_v_client_receivable_provisions" if="previous_financial_year"/>
