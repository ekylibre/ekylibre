<?xml version="1.0"?>
<procedures xmlns="http://www.ekylibre.org/XML/2013/procedures">
  <procedure name="net_removing" categories="crop_protection" actions="net_removal">
    <parameters>
      <target name="plant" filter="is plant and has indicator shape">
        <attribute name="working_zone" default-value="PRODUCT.shape" if="PRODUCT?"/>
      </target>
      <output name="protective_net" filter="can care(plant) and can cover(plant)" cardinality="*">
        <handler name="population"/>
      </output>
      <tool name="tractor" filter="can tow(equipment) and can move" cardinality="*"/>
      <tool name="net_roll" filter="can uncover(plant)" cardinality="*"/>
      <doer name="driver" filter="can drive(equipment) and can move" cardinality="*"/>
      <doer name="worker" filter="is worker" cardinality="*"/>
    </parameters>
  </procedure>
</procedures>