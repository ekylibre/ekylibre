<?xml version="1.0"?>
<procedures xmlns="http://www.ekylibre.org/XML/2013/procedures">
  <procedure name="vine_inter_row_hill_removing" categories="vine_soil_working_and_maintenance" actions="hilling" position="20">
    <parameters>
      <target name="plant" filter="is vitis and has indicator shape">
        <attribute name="working_zone" default-value="PRODUCT.shape" if="PRODUCT?" />
      </target>
      <doer name="driver" filter="is worker and can drive(equipment)" cardinality="*"/>
      <tool name="tractor" filter="is motorized_vehicle and can tow(equipment)" cardinality="*"/>
      <tool name="interrow_hill_remover"  filter="is equipment and can kill(plant)" cardinality="*"/>
    </parameters>
  </procedure>
</procedures>
