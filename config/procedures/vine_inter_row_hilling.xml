<?xml version="1.0"?>
<procedures xmlns="http://www.ekylibre.org/XML/2013/procedures">
  <procedure name="vine_inter_row_hilling" categories="vine_soil_working_and_maintenance" actions="hilling" position="10">
    <parameters>
      <target name="plant" filter="is vitis and has indicator shape">
        <attribute name="working_zone" default-value="PRODUCT.shape" if="PRODUCT?" />
      </target>
      <doer name="driver" filter="can drive(equipment) and can move" cardinality="*" />
      <tool name="tractor" filter="can tow(equipment) and can move" cardinality="*" />
      <tool name="trailed_equipment" filter="is equipment and (can earth_up or can plow or can plow_superficially)" cardinality="*" />
    </parameters>
  </procedure>
</procedures>
